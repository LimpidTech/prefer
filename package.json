{
  "name": "proto-es2015",
  "version": "0.0.0",
  "description": "Spiffy boilerplate for building and testing with ES2015+.",
  "keywords": [],
  "homepage": "https://github.com/kroogs/proto-es2015",
  "author": "Justin Krueger <justin@kroo.gs>",
  "contributors": [],
  "private": "true",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kroogs/proto-es2015.git"
  },
  "bugs": {
    "url": "https://github.com/kroogs/proto-es2015/issues"
  },
  "main": "./dist/index.js",
  "files": [
    "dist",
    "test",
    "src"
  ],
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack -p",
    "test": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "flow && prettier --no-color --trailing-comma all --no-semi --single-quote --write \"src/**/*.js\" && eslint --fix src",
    "prepublishOnly": "npm run lint"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^2.13.1",
    "eslint": "^4.2.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "file-loader": "^0.11.1",
    "flow": "^0.2.3",
    "flow-bin": "^0.49.1",
    "flow-typed": "^2.1.2",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "prettier": "^1.5.2",
    "webpack": "^3.2.0",
    "webpack-dev-server": "^2.5.1"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
